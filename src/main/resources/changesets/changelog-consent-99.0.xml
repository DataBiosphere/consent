<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="99.0" author="connorlbark">
        <addColumn tableName="institution">
            <column name="institution_url" type="text"/>
            <column name="duns_number" type="integer" defaultValue="null"/>
            <column name="org_chart_url" type="text"/>
            <column name="verification_url" type="text"/>
            <column name="verification_filename" type="varchar(255)"/>
            <column name="organization_type" type="text"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>
