<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="86.0" author="connorlbark">
        <!--Add mapping and dataset property type columns -->
        <addColumn tableName="datasetproperty">
            <column name="propertytype" type="varchar(255)"/>
            <column name="schemaproperty" type="varchar(255)"/>
        </addColumn>

        <sql>
            UPDATE datasetproperty SET propertytype='Number' WHERE propertyKey=5
        </sql>
    </changeSet>
</databaseChangeLog>
