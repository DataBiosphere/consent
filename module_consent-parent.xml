<?xml version="1.0" encoding="UTF-8"?>
<project name="module_consent-parent" default="compile.module.consent-parent">
  <dirname property="module.consent-parent.basedir" file="${ant.file.module_consent-parent}"/>
  
  <property name="module.jdk.home.consent-parent" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.consent-parent" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.consent-parent" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.consent-parent" value="-encoding UTF-8 -source 8 -target 1.8 ${compiler.args}"/>
  
  <property name="consent-parent.output.dir" value="${module.consent-parent.basedir}/target/classes"/>
  <property name="consent-parent.testoutput.dir" value="${module.consent-parent.basedir}/target/test-classes"/>
  
  <path id="consent-parent.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="consent-parent.module.production.classpath">
    <path refid="${module.jdk.classpath.consent-parent}"/>
  </path>
  
  <path id="consent-parent.runtime.production.module.classpath">
    <pathelement location="${consent-parent.output.dir}"/>
  </path>
  
  <path id="consent-parent.module.classpath">
    <path refid="${module.jdk.classpath.consent-parent}"/>
    <pathelement location="${consent-parent.output.dir}"/>
  </path>
  
  <path id="consent-parent.runtime.module.classpath">
    <pathelement location="${consent-parent.testoutput.dir}"/>
    <pathelement location="${consent-parent.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.consent-parent">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.consent-parent">
    <patternset refid="excluded.from.module.consent-parent"/>
  </patternset>
  
  
  <target name="compile.module.consent-parent" depends="compile.module.consent-parent.production,compile.module.consent-parent.tests" description="Compile module consent-parent"/>
  
  <target name="compile.module.consent-parent.production" depends="register.custom.compilers" description="Compile module consent-parent; production classes"/>
  
  <target name="compile.module.consent-parent.tests" depends="register.custom.compilers,compile.module.consent-parent.production" description="compile module consent-parent; test classes" unless="skip.tests"/>
  
  <target name="clean.module.consent-parent" description="cleanup module">
    <delete dir="${consent-parent.output.dir}"/>
    <delete dir="${consent-parent.testoutput.dir}"/>
  </target>
</project>